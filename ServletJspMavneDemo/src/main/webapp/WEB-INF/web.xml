<web-app>
	<filter>
		<filter-name>filterID</filter-name>
		<display-name>filterID</display-name>
		<description></description>
		<filter-class>com.filter.filterID</filter-class>
	</filter>
	<filter>
		<filter-name>filterName</filter-name>
		<display-name>filterName</display-name>
		<description></description>
		<filter-class>com.filter.filterName</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>filterID</filter-name>
		<url-pattern>/filterServlet</url-pattern>
	</filter-mapping>
	<filter-mapping>
		<filter-name>filterName</filter-name>
		<url-pattern>/filterServlet</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>login</servlet-name>
		<display-name>login</display-name>
		<description></description>
		<servlet-class>com.login.login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>logout</servlet-name>
		<display-name>logout</display-name>
		<description></description>
		<servlet-class>com.login.logout</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>filterID</servlet-name>
		<display-name>filterID</display-name>
		<description></description>
		<servlet-class>com.filter.filterID</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>filterServlet</servlet-name>
		<display-name>filterServlet</display-name>
		<description></description>
		<servlet-class>com.filter.servlet.filterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>filterID</servlet-name>
		<url-pattern>/filterID</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>filterServlet</servlet-name>
		<url-pattern>/filterServlet</url-pattern>
	</servlet-mapping>
